{
    "connection": {
        "type": {
            "displayName": "Proxy"
        },
        "help": {
            "type": "link",
            "default": "https://www.ibm.com/support/knowledgecenter/SSTDPP_1.1.0/docs/scp-core/data-sources-proxy.html"
        },
        "options": {
            "proxy_host": {
                "type": "text",
                "regex": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_:/\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9_:/\\-]*[A-Za-z0-9])$"
            },
            "proxy_port": {
                "type": "number",
                "default": 5000
            },
            "destination": {
                "type": "text"
            }
        }
    },
    "configuration": {
        "auth": {
            "type" : "fields",
            "notused": {
                "type": "text",
                "optional": true,
                "default": "notused"
            }
        }
    }
}