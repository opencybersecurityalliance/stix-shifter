{
    "connection": {
        "type": {
            "displayName": "Proxy"
        },
        "help": {
            "type": "link",
            "default": "data-sources.html"
        },
        "options": {
            "proxy_host": {
                "type": "text",
                "regex": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\-]*[A-Za-z0-9])$|^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$"
            },
            "proxy_port": {
                "type": "number",
                "default": 5000,
                "min": 1,
                "max": 65535
            },
            "proxy_cert": {
                "type": "password",
                "optional": true,
                "hidden": true
            },
            "destination": {
                "type": "text"
            }
        }
    },
    "configuration": {
        "auth": {
            "type" : "fields",
            "notused": {
                "type": "text",
                "optional": true,
                "default": "notused"
            }
        }
    }
}