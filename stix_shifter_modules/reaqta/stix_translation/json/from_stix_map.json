{
  "directory": {
    "fields": {
      "path": ["path"]
    }
  },
  "file": {
    "fields": {
      "hashes.'MD5'": ["md5"],
      "hashes.'SHA-1'": ["sha1"],
      "hashes.'SHA-256'": ["sha256"],
      "name": ["filename"],
      "parent_directory_ref.path": ["path"]
    }
  },
  "ipv4-addr": {
    "fields": {
      "value": ["ip"]
    }
  },
  "ipv6-addr": {
    "fields": {
      "value": ["ip"]
    }
  },
  "network-traffic": {
    "fields": {
      "dst_port": ["eventdata.remotePort"],
      "dst_ref.value": ["ip"],
      "src_port": ["eventdata.localPort"],
      "src_ref.value": ["ip"]
    }
  },
  "process": {
    "fields": {
      "binary_ref.name": ["filename"],
      "creator_user_ref.user_id": ["user.startsWith"],
      "parent_ref.binary_ref.name": ["ppid"],
      "pid": ["pid", "ppid"]
    }
  },
  "user-account": {
    "fields": {
      "user_id": ["user", "user.startsWith"]
    }
  },
  "url": {
    "fields": {
      "value": ["eventdata.url"]
    }
  },
  "x-ibm-finding": {
    "fields": {
      "dst_ip_ref.value": ["ip"],
      "finding_type": ["eventType"],
      "src_ip_ref.value": ["ip"]
    }
  },
  "x-oca-asset": {
    "fields": {
      "ip_refs[*].value": ["ip"]
    }
  },
  "x-oca-event": {
    "fields": {
      "code": ["eventId"],
      "file_ref.name": ["path"],
      "network_ref.src_ref.value": ["ip"],
      "parent_process_ref.pid": ["ppid"],
      "process_ref.pid": ["pid"],
      "user_ref.user_id": ["user.startsWith"]
    }
  },
  "x-reaqta-cert": {
    "fields": {
      "expired": ["expired"],
      "issuer": ["issuer"],
      "signer": ["signer"],
      "trusted": ["trusted"]
    }
  },
  "x-reaqta-event": {
    "fields": {
      "endpoint_id": ["endpointId"]
    }
  },
  "x-reaqta-network": {
    "fields": {
      "outbound": ["isOutbound"]
    }
  },
  "x-reaqta-process": {
    "fields": {
      "logon_id": ["login.id"],
      "no_gui": ["hasGui"],
      "privilege_level": ["privilege"],
      "user_sid": ["user.sid"]
    }
  },
  "x-reaqta-program": {
    "fields": {
      "arch": ["arch"],
      "fsnamee": ["filename"]
    }
  }
}