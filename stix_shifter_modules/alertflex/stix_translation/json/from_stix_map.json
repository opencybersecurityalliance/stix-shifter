{
  "ipv4-addr": {
    "fields": {
      "src_ip": ["a.srcip"],
      "dst_ip": ["a.dstip"],
      "ip": ["a.dstip", "a.srcip"]
    }
  },
  "file": {
    "fields": {
      "name": ["a.file"]
    }
  },
  "user": {
    "fields": {
      "name": ["a.user"]
    }
  },
  "host": {
    "fields": {
      "agent": ["a.agent", "a.srcagent", "a.dstagent"]
    }
  },
  "process" : {
    "fields": {
      "name": ["a.process"]
    }
  },
  "alert" : {
    "fields": {
      "id": ["a.event"],
      "category": ["a.category"],
      "severity": ["a.severity"]
    }
  }
}
